    "react": "16.4.0",
    "react-dnd": "2.6.0",
    "react-dnd-html5-backend": "4.0.4",
    "react-dom": "16.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.2",
    "redux-observable": "1.0.0-alpha.3",
